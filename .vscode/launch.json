    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/moses",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [
                {
                    "name": "DYLD_FALLBACK_LIBRARY_PATH",
                    "value": "${workspaceFolder}/jemalloc/lib"
                },
                {
                    "name": "JE_MALLOC_CONF",
                    "value": "narenas:1"
                },
            ],
            "externalConsole": false,
            "MIMode": "lldb"
        }
    ]
}